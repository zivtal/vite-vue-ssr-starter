{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx tsx server/server.ts",
    "build": "rm -rf dist && npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest .vite/ssr-manifest.json --outDir dist/client",
    "build:server": "vite build --ssr client/entry-server.ts --outDir dist/server",
    "serve": "NODE_ENV=production npx tsx server/server.ts",
    "preview": "vite preview",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && yarn.lock && npm install"
  },
  "dependencies": {
    "axios": "1.6.0",
    "cheerio": "^1.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "memorystore": "^1.6.7",
    "prettier": "^3.3.3",
    "pwa-install-handler": "^2.5.0",
    "register-service-worker": "^1.7.2",
    "sirv": "^2.0.4",
    "tiny-emitter": "^2.1.0",
    "uuid": "^9.0.1",
    "vue": "^3.4.35",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.2.0",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-vue": "^9.26.0",
    "sass-embedded": "^1.77.8",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.20.1",
    "vite-plugin-vue-devtools": "^7.3.8",
    "vue-tsc": "^2.0.29"
  }
}
